#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,arr[n];
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    //pre compute
    int hash[13]={0};
    for(int i=0;i<n;i++){
        hash[arr[i]]+=1;
    }
    
    //Query statements
    int number;
    cout<<"enter the number to check frequency";
    cin>>number;
    cout<<hash[number]<<endl;
}
